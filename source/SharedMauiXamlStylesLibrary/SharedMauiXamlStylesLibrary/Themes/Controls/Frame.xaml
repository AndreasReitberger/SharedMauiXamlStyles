<?xml version="1.0" encoding="utf-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    >
    <!-- OPTIMIZED -->
    <!-- Frames -->
    <Style x:Key="ListViewItemFrameStyle" TargetType="Frame">
        <Setter Property="BorderColor" Value="{DynamicResource TransparentColor}"/>
        <Setter Property="BackgroundColor" Value="{DynamicResource TransparentColor}" />
        <Setter Property="BorderColor" Value="{DynamicResource ItemTemplateBorder}" />
    </Style>
</ResourceDictionary>